#include <iostream>
#include <vector>
using  namespace std;

int main()
{
    vector<int> v ;
    for (int i = 1; i < 10; i++)
    {
        v.push_back(i);

    }
    cout << "Size of the Vector " << v.size() << endl;
    v.resize(7);
    cout << "Size of the Vector after resizing " << v.size() << endl;

    if (v.empty() == false)
    {
        cout << "\n not empty ";
    }
    else
    {
        cout << "\n it is empty ";
    }
    cout << "\n Elements of vector ";
    for (auto  it = v.begin(); it != v.end(); it++)
    {
        cout << *it << " ";
    }
    return 0;
}



